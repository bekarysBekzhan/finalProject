Table USERS(ID INT NOT NULL AUTO_INCREMENT, NAME VARCAHR(100) NOT NULL, EMAIL VARCHAR(100) NOT NULL, PASSWORD VARCHAR(100) NOT NULL, PRIMARY KEY(ID))
PRODUCTS(ID INT NOT NULL AUTO_INCREMENT,NAME VARCHAR(100) NOT NULL,PRICE INT NOT NULL, IMAGE VARCHAR(100) NOT NULL)
CART(ID INT NOT NULL AUTO_INCREMENT, USER_ID INT NOT NULL, NAME VARCHAR(100) NOT NULL,PRICE INT NOT NUL,QUANTITY INT NOT NULL,IMAGE VARCHAR(100) NOT NULL)
